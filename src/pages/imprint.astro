---
import Layout from '../layouts/Layout.astro';
---
<Layout title="Imprint | Deczer Solution">
	<main class="max-w-3xl mx-auto px-6 py-20">
		<h1 class="text-4xl font-bold mb-10 text-white">Imprint</h1>
		<div class="prose prose-invert text-slate-400">
			<p>Responsible for the content of this website:</p>
			<p class="text-white mt-4 italic">[Your Full Name]<br/>[Your Street]<br/>[Your City/ZIP]<br/>[Your Email Address]</p>
		</div>
	</main>
</Layout>